{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "shell-server",
  "sourceRoot": "src/shell-app/server",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "src/shell-app/server",
        "command": "npm run build"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "src/shell-app/server",
        "command": "npm run start"
      }
    }
  },
  "tags": []
}
